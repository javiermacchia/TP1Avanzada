<%@ Page Title="Login" Language="C#" MasterPageFile="~/Site.Master"
         AutoEventWireup="true" CodeBehind="Login.aspx.cs"
         Inherits="TP1Avanzada.Login" %>

<asp:Content ID="BodyContent" ContentPlaceHolderID="MainContent" runat="server">
    <link href="~/Content/Site.css" rel="stylesheet" />

  <div class="container">
    <div class="login-wrapper">
      <img class="login-icon"
           src="data:image/jpeg;base64,"
           alt="Icono de usuario" />

      <div class="form-group">
        <asp:Label runat="server" AssociatedControlID="Txusuario">Usuario:</asp:Label>
        <asp:TextBox ID="Txusuario" CssClass="form-control" runat="server" />
      </div>

      <div class="form-group mt-3">
        <asp:Label runat="server" AssociatedControlID="TxPassword">Password:</asp:Label>
        <asp:TextBox ID="TxPassword"
                     TextMode="Password"
                     CssClass="form-control"
                     runat="server" />
      </div>

      <div class="form-group mt-4">
        <asp:Button ID="BtLogin"
                    runat="server"
                    Text="Ingresar"
                    CssClass="btn btn-primary w-100"
                    OnClick="BtLogin_Click" />
      </div>

      <div class="d-flex justify-content-between mt-3">
        <asp:HyperLink runat="server" NavigateUrl="~/Register.aspx">Regístrate</asp:HyperLink>
        <asp:HyperLink runat="server" NavigateUrl="~/Recovery.aspx">Recuperar contraseña</asp:HyperLink>
      </div>

      <div class="mt-3 text-center">
        <asp:Label ID="lt_mensaje"
                   runat="server"
                   Visible="false"
                   CssClass="text-danger" />
      </div>
    </div>
  </div>
</asp:Content>
